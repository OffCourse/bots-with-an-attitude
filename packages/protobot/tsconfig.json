{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "outDir": "dist",
    "allowJs": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*"],
  "lib": ["es2015"],
  "exclude": [    
    "**/@tensorflow",
    "**/@tensorflow/**",
    "**@tensorflow/tfjs",
    "**@tensorflow/tfjs**"
  ]
}
